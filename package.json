{
  "name": "rule-angular",
  "version": "0.0.1",
  "description": "Starter for builders",
  "author": "Leonid Romanov <romanov.leonid@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "main": "src/app.js",
  "scripts": {
    "build-dev": "rm -rf ./dist/*.* && cross-env NODE_ENV=development --profile --display-modules --display-reasons",
    "build-test": "rm -rf ./dist/*.* && cross-env NODE_ENV=test webpack --progress --hide-modules",
    "build-dist": "rm -rf ./dist/*.* && cross-env NODE_ENV=production webpack --progress --hide-modules",
    "run": "webpack-dev-server --history-api-fallback --hot --inline --progress",
    "run-test": "cross-env NODE_ENV=test webpack-dev-server --history-api-fallback --hot --inline --progress",
    "eslint": "eslint src/ --ext .js",
    "test": "npm run eslint && karma start --single-run",
    "build": "rm -rf ./dist/*.* && webpack --config webpack.build.js --bail -p",
    "dev": "node node_modules/.bin/webpack-dev-server --history-api-fallback --hot --inline --progress",
    "outdated": "npm outdated --depth 0",
    "karma": "cross-env NODE_ENV=test karma start  --single-run",
    "karma:live": "karma start --auto-watch --no-single-run",
    "start": "node src/server"
  },
  "pre-commit": [
    "eslint",
    "test",
    "outdated"
  ],
  "bugs": {
    "url": ""
  },
  "dependencies": {
    "angular": "^1.5.0",
    "angular-animate": "^1.5.2",
    "angular-aria": "^1.4.8",
    "angular-cache": "^4.5.0",
    "angular-messages": "^1.4.8",
    "angular-schema-form": "^0.8.12",
    "angular-ui-router": "^1.0.0-alpha0",
    "bootstrap": "^3.3.6",
    "d3": "^3.5.16",
    "immutable": "^3.7.6",
    "lokijs": "^1.3.15",
    "mongoose": "^4.4.6",
    "node-libs-browser": "*",
    "satellizer": "^0.14.0",
    "urijs": "^1.17.1"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.3",
    "autoprefixer-loader": "^3.2.0",
    "babel": "*",
    "babel-core": "*",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "eslint": "^2.2.0",
    "eslint-loader": "^1.3.0",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.6.2",
    "http-server": "^0.8.5",
    "image-webpack-loader": "^1.6.3",
    "isparta-instrumenter-loader": "^1.0.0",
    "jasmine-async-sugar": "^1.3.3",
    "jasmine-core": "*",
    "karma": "^0.13.19",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.8",
    "karma-junit-reporter": "^0.3.8",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.24",
    "karma-webpack": "^1.7.0",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.4",
    "protractor": "^3.0.0",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.13.0",
    "stylus-loader": "^1.5.1",
    "url-loader": "*",
    "webpack": "^1.12.14",
    "webpack-dev-server": "*"
  }
}
